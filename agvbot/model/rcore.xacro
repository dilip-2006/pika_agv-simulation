<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:include filename="inertial.xacro" />


    <material name="white">
        <color rgba="1 1 1 1" />
    </material>
    <material name="orange">
        <color rgba="1 0.3 0.1 1"/>
    </material>
    <material name="blue">
        <color rgba="0.2 0.2 1 1"/>
    </material>
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>
    <material name="dark_gray">
        <color rgba="0.2 0.2 0.2 1.0"/>
    </material>
    <material name="light_gray">
        <color rgba="0.7 0.7 0.7 1.0"/>
    </material>
    <material name="silver">
        <color rgba="0.8 0.8 0.8 1.0"/>
    </material>
    <material name="yellow">
        <color rgba="1.0 0.8 0.0 1.0"/>
    </material>


    <!-- BASE LINK -->

    <link name="base_link">

    </link>


    <!-- CHASSIS LINK -->

    <joint name="chassis_joint" type="fixed">
        <parent link="base_link"/>
        <child link="chassis"/>
        <origin xyz="-0.499 0 0"/>
    </joint>

    <link name="chassis">
        <visual>
            <origin xyz="0.5 0 0.1"/>
            <geometry>
                <box size="1 0.6 0.2"/>
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin xyz="0.5 0 0.1"/>
            <geometry>
                <box size="1 0.6 0.2"/>
            </geometry>
            <material name="white"/>
        </collision>
        <xacro:inertial_box mass="0.1" x="1.0" y="0.6" z="0.2">
            <origin xyz="0.5 0 0.1" rpy="0 0 0"/>
        </xacro:inertial_box>

    </link>

    <gazebo reference="chassis">
        <material>Gazebo/White</material>
    </gazebo>

    <!-- LEFT WHEEL LINK -->


    <joint name="left_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin xyz="0 0.325 0" rpy="1.57079632679 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="left_wheel">

        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <material name="dark_gray"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <material name="dark_gray"/>
        </collision>
        <xacro:inertial_cylinder mass="0.1" length="0.05" radius="0.1">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="left_wheel">
        <material>Gazebo/dark_gray</material>
    </gazebo>

    <!-- RIGHT WHEEL LINK -->


    <joint name="right_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin xyz="0 -0.325 0" rpy="1.57079632679 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="right_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <material name="dark_gray"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <material name="dark_gray"/>
        </collision>
        <xacro:inertial_cylinder mass="0.1" length="0.05" radius="0.1">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="right_wheel">
        <material>Gazebo/dark_gray</material>
    </gazebo>

    <!-- CASTER WHEEL LINK -->


    <joint name="front_left_caster_joint" type="fixed">
        <parent link="base_link"/>
        <child link="front_left_caster"/>
        <origin xyz="0.45 0.275 -0.05" rpy="0 0 0"/>
    </joint>

    <link name="front_left_caster">
        <visual>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="yellow"/>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="yellow"/>
        </collision>
        <xacro:inertial_sphere mass="0.1" radius="0.05">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_sphere>
    </link>

    <gazebo reference="front_right_caster">
        <material>Gazebo/Yellow</material>
    </gazebo>

    <joint name="front_right_caster_joint" type="fixed">
        <parent link="base_link"/>
        <child link="front_right_caster"/>
        <origin xyz="0.45 -0.275 -0.05" rpy="0 0 0"/>
    </joint>

    <link name="front_right_caster">
        <visual>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="yellow"/>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="yellow"/>
        </collision>
        <xacro:inertial_sphere mass="0.1" radius="0.05">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_sphere>
    </link>

    <gazebo reference="front_right_caster">
        <material>Gazebo/Yellow</material>
    </gazebo>

    <joint name="rear_right_caster_joint" type="fixed">
        <parent link="base_link"/>
        <child link="rear_right_caster"/>
        <origin xyz="-0.45 -0.275 -0.05" rpy="0 0 0"/>
    </joint>

    <link name="rear_right_caster">
        <visual>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="yellow"/>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="yellow"/>
        </collision>
        <xacro:inertial_sphere mass="0.1" radius="0.05">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_sphere>
    </link>

    <gazebo reference="rear_right_caster">
        <material>Gazebo/Yellow</material>
    </gazebo>

    <joint name="rear_left_caster_joint" type="fixed">
        <parent link="base_link"/>
        <child link="rear_left_caster"/>
        <origin xyz="-0.45 0.275 -0.05" rpy="0 0 0"/>
    </joint>

    <link name="rear_left_caster">
        <visual>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="yellow"/>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="yellow"/>
        </collision>
        <xacro:inertial_sphere mass="0.1" radius="0.05">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_sphere>
    </link>

    <gazebo reference="rear_left_caster">
        <material>Gazebo/Yellow</material>
    </gazebo>

    <!-- LIDAR LINK -->

    <link name="lidar_link">
        <visual>
            <geometry>
                <cylinder radius="0.05" length="0.05"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.05" length="0.05"/>
            </geometry>
            <material name="blue"/>
        </collision>
    </link>

    <joint name="lidar_joint" type="fixed">
        <parent link="base_link"/>
        <child link="lidar_link"/>
        <origin xyz="0.4 0 0.22" rpy="0 0 0"/>
    </joint>

    <gazebo reference="lidar_link">
        <material>Gazebo/Blue</material>
    </gazebo>
</robot>